@model MovieRental.ViewModel.CustomerListViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table table-hover">
    <col width="5%">
    <col width="95%">
    <tr class="table-primary">
        <th>ID</th>
        <th>Title</th>
    </tr>
    @foreach (var Customer in Model.CustomerList)
    {
        <tr scope="row">
            <td>@Customer.Id</td>
            <td>
                <a href="/Customer/Details/@System.Convert.ToInt32(Customer.Id)">
                    @Customer.Name
                </a>
            </td>
            @if (Customer.Id != null)
            {
                Console.WriteLine(Customer.Id);
            }
        </tr>
    }

</table>
